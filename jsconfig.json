{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
      "allowJs": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "skipLibCheck": false,
      "sourceMap": true,
      "strict": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}
